<%= form_tag(search_result_path, method: "get") do %> 
  <select name="category" class="form-control" id="exampleFormControlSelect1">
    <option value="1">제목</option>
    <option value="2">작성자</option>
  </select>

  <%= label_tag(:div, "검색 단어 :") %>
  <%= text_field_tag(:search_text) %> 
  <%= submit_tag("검색") %>
<% end %>